<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication1</title>
    <meta name="author" content="Hector Contreras" />
    <link href="~/css/styles.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/js/all.min.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body class="sb-nav-fixed">
    <!-- navbar side -->
    <!-- nombre y demas cosillas de la barra lateral -->
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <a class="navbar-brand" href="/swagger/index.html">Web Attendance</a>
        <a class="btn btn-link btn-sm order-1 order-lg-0 color--btn__Link" target="_blank" href="/swagger/index.html"><i class="fas fa-book"></i></a>
        <button class="btn btn-link btn-sm order-1 order-lg-0" id="sidebarToggle" href="#"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form hidden class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
            <div class="input-group">
                <input hidden class="form-control" type="text" placeholder="Search for..." aria-label="Search" aria-describedby="basic-addon2" />
                <div class="input-group-append">
                    <button hidden class="btn btn-primary" type="button"><i class="fas fa-search"></i></button>
                </div>
            </div>
        </form>
        <!-- Navbar -->
        <ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="userDropdown" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                    <a class="dropdown-item" href="#">Settings</a>
                    <a class="dropdown-item" href="#">Activity Log</a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="login.html">Logout</a>
                </div>
            </li>
        </ul>
    </nav>
    <!-- end side -->
    <!-- nav horizontal va con el cuerpo -->
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Core</div>
                        <a class="nav-link" asp-action="Index" asp-controller="Home">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Pagina Principal
                        </a>

                        <div class="sb-sidenav-menu-heading">Accesos</div>
                        <!-- ############################## -->
                        <!--esto es una subseccion-->
                        <!-- ############################## -->
                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayouts" aria-expanded="false" aria-controls="collapseLayouts">
                            <div class="sb-nav-link-icon"><i class="fas fa-air-freshener"></i></div>
                            Feriados
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayouts" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="Feriados">Listado</a>
                                <a class="nav-link" asp-action="Insert" asp-controller="Feriados">Nuevo</a>
                            </nav>
                        </div>
                        <!-- ############################## -->
                        <!--aqui termina la subseccion-->
                        <!-- ############################## -->

                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayoutsEmpresas" aria-expanded="false" aria-controls="collapseLayoutsEmpresas">
                            <div class="sb-nav-link-icon"><i class="fas fa-building"></i></div>
                            Empresas
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayoutsEmpresas" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="Empresas">Listado</a>
                                <a class="nav-link" asp-action="Insert" asp-controller="Empresas">Nuevo</a>
                            </nav>
                        </div>
                        <!-- ############################## -->
                        <!--aqui termina la subseccion-->
                        <!-- ############################## -->

                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayoutsHorarios" aria-expanded="false" aria-controls="collapseLayoutsHorarios">
                            <div class="sb-nav-link-icon"><i class="far fa-calendar-alt"></i></div>
                            Horarios
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayoutsHorarios" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="Horarios">Listado</a>
                                <a class="nav-link" asp-action="Insert" asp-controller="Horarios">Nuevo</a>
                            </nav>
                        </div>
                        <!-- ############################## -->
                        <!--aqui termina la subseccion-->
                        <!-- ############################## -->
                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayoutsPersonass" aria-expanded="false" aria-controls="collapseLayoutsPersonass">
                            <div class="sb-nav-link-icon"><i class="fas fa-user-friends"></i></div>
                            Personas
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayoutsPersonass" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="Personas">Listado</a>
                                <a class="nav-link" asp-action="Insert" asp-controller="Personas">Nuevo</a>
                            </nav>
                        </div>
                        <!-- ############################## -->
                        <!--aqui termina la subseccion-->
                        <!-- ############################## -->
                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayoutsEmployed" aria-expanded="false" aria-controls="collapseLayoutsEmployed">
                            <div class="sb-nav-link-icon"><i class="fas fa-briefcase"></i></div>
                            Empleados
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayoutsEmployed" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="Empleados">Listado</a>
                                <a class="nav-link" asp-action="Insert" asp-controller="Empleados">Nuevo</a>
                            </nav>
                        </div>
                        <!-- ############################## -->
                        <!--aqui termina la subseccion-->
                        <!-- ############################## -->
                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayoutsAdministradorDeHorarios" aria-expanded="false" aria-controls="collapseLayoutsAdministradorDeHorarios">
                            <div class="sb-nav-link-icon"><i class="far fa-calendar-check"></i></div>
                            Admo. Horarios
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayoutsAdministradorDeHorarios" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="AdministradorDeHorarios">Listado</a>
                                <a class="nav-link" asp-action="Insert" asp-controller="AdministradorDeHorarios">Nuevo</a>
                            </nav>
                        </div>

                        <!-- ############################## -->
                        <!--aqui termina la subseccion-->
                        <!-- ############################## -->
                        <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayoutsReporte" aria-expanded="false" aria-controls="collapseLayoutsReporte">
                            <div class="sb-nav-link-icon"><i class="far fa-file-alt"></i></div>
                            Reportes
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayoutsReporte" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-action="Index" asp-controller="Reporte">Listado</a>
                                @*<a class="nav-link" asp-action="Insert" asp-controller="Reporte">Nuevo</a>*@
                            </nav>
                        </div>



                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">Para Dudas escribenos a: </div>
                    desarrollo@autolau.cl
                </div>
            </nav>
        </div>


        <!-- ############################ -->
        <!-- aqui esta mi cuerpo principal-->
        <!-- ############################ -->

        <div id="layoutSidenav_content" class="container-fluid">
            <main role="main">
                <div class="container m-3 align-items-center justify-content-center text-center center">
                    @RenderBody()
                </div>
            </main>
        </div>
    </div>




